{
  "name": "2020react-portfolio",
  "version": "1.0.0",
  "description": "portfolio with react",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node scripts/seedDB.js",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ruffmantom/2020React-portfolio.git"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.7"
  },
  "bugs": {
    "url": "https://github.com/Ruffmantom/2020React-portfolio/issues"
  },
  "homepage": "https://github.com/Ruffmantom/2020React-portfolio#readme",
  "dependencies": {
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "if-env": "^1.0.4",
    "nodemailer": "^6.4.6",
    "nodemailer-mailgun-transport": "^2.0.0"
  }
}
